////////////									BEGIN PART 1							////////////
////////////																			////////////
////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////
         
BEGIN @1
REQUIRE_PREDICATE GAME_IS ~bgt bgee tob bg2ee tutu_totsc eet~ @2



ACTION_IF FILE_EXISTS_IN_GAME ~ACVIRGI.CRE~
THEN BEGIN
PRINT @3
COMPILE ~AC_QUEST/Virgi/ACVIRGI2.D~
	
	ACTION_IF GAME_IS ~bgt eet~ THEN BEGIN
EXTEND_TOP ~ACVIRGI.bcs~ ~AC_QUEST/Virgi/ACVIRGI.BAF~
		EVALUATE_BUFFER
	END
	
END ELSE BEGIN 
PRINT @4
COPY ~AC_QUEST/cre/ACVIRGI.CRE~ ~OVERRIDE/ACVIRGI.CRE~
SAY NAME1 ~~
SAY NAME2 ~~
WRITE_ASCII 0x280 ~ACVIRGI~
WRITE_ASCII 0x2cc ~ACVIRGI~
WRITE_ASCII 0x248 ~ACVIRGI~
COMPILE ~AC_QUEST/Virgi/ACVIRGI.D~
	
	ACTION_IF GAME_IS ~bgt eet~ THEN BEGIN
COMPILE ~AC_QUEST/Virgi/ACVIRGI.BAF~
		EVALUATE_BUFFER
	END
	
END

ACTION_IF GAME_IS ~bgt eet~
THEN BEGIN
EXTEND_BOTTOM ~AR0602.bcs~ ~AC_QUEST/BAF/AC0602.BAF~
END

ACTION_IF GAME_IS ~bgt bgee tutu_totsc eet~ THEN BEGIN

ACTION_IF GAME_IS ~bgee~ THEN BEGIN
COPY ~AC_QUEST/TRA/%LANGUAGE%/sounds~ ~override~
END


