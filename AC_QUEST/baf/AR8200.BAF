IF
	OnCreation()
THEN
	RESPONSE #100
		TriggerActivation("ACDOOR01",TRUE)
		TriggerActivation("DOOR8201",FALSE)
		Continue()
END

IF
	Global("ACQ1NASHKELL","GLOBAL",1)
THEN
	RESPONSE #100
		SetGlobal("ACQ1NASHKELL","GLOBAL",2)
		TriggerActivation("ACDOOR01",FALSE)
		TriggerActivation("DOOR8201",TRUE)
		Continue()
END